:root {
	/* Background Colors */
	--jquery-datepicker-bgd-color: var(--color-white);
	--jquery-datepicker-bgd-color-secondary: var(--color-white);
	--jquery-datepicker-bgd-color-tertiary: var(--color-white);
	--jquery-datepicker-bgd-color-quaternary: var(--color-white);
	--jquery-datepicker-bgd-color-active: var(--color-mako-alt);

	/* Colors */
	--jquery-datepicker-color: var(--color-nevada);
	--jquery-datepicker-color-option: var(--color-nevada);
	--jquery-datepicker-color-disabled: rgba(255, 255, 255, 0.65);

	/* Fonts */
	--jquery-datepicker-font-size: 16px;
	--jquery-datepicker-font-style: var(--form-control-font-style);
	--jquery-datepicker-font-weight: var(--form-control-font-weight);
	--jquery-datepicker-line-height: var(--form-control-height);
	--jquery-datepicker-font-family: var(--form-font-family);
}
